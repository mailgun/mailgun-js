    "popsicle": "^1.1.1",
    "popsicle-status": "^0.2.0",
    "url-join": "0.0.1",
    "webpack-merge": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.1.0",
    "eslint": "^7.12.1",
